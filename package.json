{
  "name": "audit-trail-scraper",
  "version": "0.3.1",
  "private": true,
  "description": "Developer Hackathon",
  "author": "Ganesh Shankar",
  "license": "UNLICENSED",
  "scripts": {
    "start:web:dev": "concurrently \"nodemon --config nodemon/nodemon-web.json\" \"npm run start:client:dev\"",
    "start:client:dev": "node start-client.js",
    "start:web:prod": "cross-var node -r ts-node/register --optimize_for_size --max-semi-space-size=128 --max_old_space_size=$MAX_OLD_SPACE_SIZE src/workers/web/main.ts",
    "format": "eslint --fix ./src/**/*.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --runInBand --forceExit --collectCoverageFrom=./**/*.ts --collectCoverageFrom=!./test/*.ts --collectCoverageFrom=!./migrations/*.ts",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "dbs": "docker-compose --compatibility up",
    "logs:dev:tail": "heroku logs --app faststart-heroku-deloitte-dev --tail",
    "heroku-postbuild": "cd client && npm install && npm run build && cd .. && npm run build:react:copy-assets",
    "build:react:copy-assets": "node scripts/copy-reactjs-build.js"
  },
  "dependencies": {
    "@nest-middlewares/compression": "^6.0.0",
    "@nest-middlewares/connect-timeout": "^6.0.0",
    "@nest-middlewares/csurf": "^6.0.0",
    "@nest-middlewares/express-session": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/platform-socket.io": "^6.8.5",
    "@nestjs/platform-ws": "^6.10.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.8.5",
    "bluebird": "^3.7.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "connect-redis": "^3.4.2",
    "cross-env": "^5.2.1",
    "cross-var": "^1.1.0",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-rate-limit": "^3.5.3",
    "express-session": "^1.17.0",
    "express-socket.io-session": "^1.3.5",
    "handlebars-intl": "^1.1.2",
    "helmet": "^3.21.2",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.6.0",
    "ioredis": "^4.14.1",
    "ioredis-mock": "^4.18.2",
    "jsforce": "^1.9.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nest-schedule": "^0.6.3",
    "passport": "^0.4.0",
    "passport-saml": "^1.2.0",
    "pg": "^7.12.1",
    "redux-socket.io": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "socket.io-client": "^2.3.0",
    "ts-node": "^8.4.1",
    "typeorm": "^0.2.20",
    "underscore.string": "^3.3.5",
    "json-rules-engine": "5.0.4",
    "memory-cache": "0.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.8.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.13",
    "@types/jsdom": "^12.2.4",
    "@types/jsforce": "^1.9.11",
    "@types/node": "^10.17.0",
    "@types/passport": "^1.0.1",
    "@types/redis": "^2.8.14",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "concurrently": "^5.0.0",
    "copyfiles": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jsdom": "^15.2.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.1.0",
    "supertest": "^3.4.1",
    "ts-node": "^8.4.1",
    "ts-jest": "^24.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "lodash": "4.17.12"
  },
  "engines": {
    "node": "12.13.x"
  }
}
